<script>

 $(function() {
   var image = [];
   $.getJSON('image.json', function(data) {
       $.each(data.image_data, function(i, f) {
          var tblRow = "<tr>" + "<td>" + f.firstName + "</td>" +
           "<td>" + f.lastName + "</td>" + "<td>" + f.job + "</td>" + "<td>" + f.roll + "</td>" + "</tr>"
           $(tblRow).appendTo("#userdata tbody");
     });

   });

});
</script>


<script> 
var i1=0 , i7 =0;
var i2=0 , i4=0;
var i3=0 , i5=0;
var i6=0 , i8=0;
var cc=0;
function change(panel,image)
{	
	document.getElementById("msg").innerHtml="hello";	
	if(image==1)i1++;
	if(image==2)i2++;
	if(image==3)i3++;
	if(image==4)i4++;
	if(image==5)i5++;
	if(image==6)i6++;
	if(image==7)i7++;
	if(image==8)i8++;

	if((i1==2 && i7==2)||(i1==4 && i7==4)||(i1==6 && i7==6)){document.getElementById("flip1").style.display="none";document.getElementById("flip7").style.display="none";}
	if((i2==2 && i4==2)||(i2==4 && i4==4)||(i2==6 && i4==6)){document.getElementById("flip2").style.display="none";document.getElementById("flip4").style.display="none";}
	if((i3==2 && i5==2)||(i3==4 && i5==4)||(i3==6 && i5==6)){document.getElementById("flip3").style.display="none";document.getElementById("flip5").style.display="none";}
	if((i6==2 && i8==2)||(i6==4 && i8==4)||(i6==6 && i8==6)){document.getElementById("flip6").style.display="none";document.getElementById("flip8").style.display="none";}
	
	
 $(document).ready(function(){
  
    	$(panel).slideDown("slow");
		$(panel).slideUp("slow");
 	
    });



}
</script>
 
