create table assesment1(
id int NOT NULL AUTO_INCREMENT,
name VARCHAR(15) NOT NULL,
passmarks TINYINT(3) NOT NULL,
created DATE NOT NULL,
modified DATETIME NOT NULL,
PRIMARY KEY(id)
);




