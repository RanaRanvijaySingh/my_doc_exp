create table employee(
id TINYINT UNSIGNED NOT NULL AUTO_INCREMENT,
fname VARCHAR(15) NOT NULL,
age TINYINT(3) UNSIGNED NOT NULL,
dob DATE NOT NULL,
dept_id TINYINT UNSIGNED NOT NULL,
PRIMARY KEY(id),
FOREIGN KEY (dept_id) REFERENCES department(id)
);


